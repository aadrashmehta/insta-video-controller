{
  "manifest_version": 3,
  "name": "Insta Video Controls",
  "description": "Injects a custom seek controller synced with videos.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ]
}
